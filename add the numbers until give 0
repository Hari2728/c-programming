#include <stdio.h>
void main()
{
    int a,sum=0;
    while(1)
    {
        printf("Enter Number:");
        scanf("%d",&a);
        if(a<100)
        sum=a+sum;
        if(a==0)
        break;
    }
    printf("sum of number given :%d",sum);
}

output
Enter Number:30
Enter Number:12
Enter Number:22
Enter Number:0
sum of number given :64
